CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `asignarValor`(in op varchar(1),in macro int,in empleado int,in proceso int,in f1 date,in f2 date,in c1 int,in c2 int)
BEGIN
DECLARE total FLOAT;
DECLARE total1 FLOAT;
declare total2 float;
SET total=(SELECT DISTINCT @total := valor FROM escuela_proceso WHERE cod_macroproceso=macro);
CASE op 
WHEN '1' THEN 
begin
if exists (select COD_EMPLEADO from indicador where cod_empleado=empleado and COD_PROCESO=proceso and COD_MACROPROCESO=macro and FECHA_INICIO=f1 and FECHA_FIN=f2) then
UPDATE indicador SET porcentaje=@total WHERE cod_empleado=empleado AND COD_PROCESO=proceso AND COD_MACROPROCESO=macro AND FECHA_INICIO=f1 AND FECHA_FIN=f2;
else
INSERT INTO indicador(COD_EMPLEADO,COD_PROCESO,PORCENTAJE,COD_MACROPROCESO,FECHA_INICIO,FECHA_FIN) VALUES (empleado,proceso,@total,macro,f1,f2);
end if;
end;
COMMIT; 
WHEN '0' THEN 
begin
SET total1=0;
IF EXISTS (SELECT COD_EMPLEADO FROM indicador WHERE cod_empleado=empleado AND COD_PROCESO=proceso AND COD_MACROPROCESO=macro AND FECHA_INICIO=f1 AND FECHA_FIN=f2) THEN
UPDATE indicador SET porcentaje=total1 WHERE cod_empleado=empleado AND COD_PROCESO=proceso AND COD_MACROPROCESO=macro AND FECHA_INICIO=f1 AND FECHA_FIN=f2;
ELSE
INSERT INTO indicador(COD_EMPLEADO,COD_PROCESO,PORCENTAJE,COD_MACROPROCESO,FECHA_INICIO,FECHA_FIN) VALUES (empleado,proceso,total1,macro,f1,f2);
END IF;
end;
WHEN '2' THEN 
BEGIN
SET total2=((c2*total)/c1);
IF EXISTS (SELECT COD_EMPLEADO FROM indicador WHERE cod_empleado=empleado AND COD_PROCESO=proceso AND COD_MACROPROCESO=macro AND FECHA_INICIO=f1 AND FECHA_FIN=f2) THEN
UPDATE indicador SET porcentaje=total2 WHERE cod_empleado=empleado AND COD_PROCESO=proceso AND COD_MACROPROCESO=macro AND FECHA_INICIO=f1 AND FECHA_FIN=f2;
ELSE
INSERT INTO indicador(COD_EMPLEADO,COD_PROCESO,PORCENTAJE,COD_MACROPROCESO,FECHA_INICIO,FECHA_FIN) VALUES (empleado,proceso,total2,macro,f1,f2);
END IF;
END;
end case; 
  END$$

