CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `op_empleado`(IN op VARCHAR(1),IN id INT,IN ci VARCHAR(10),IN nom VARCHAR(40),IN sex varchar(1), mail VARCHAR(200),IN cel1 varchar(20),IN cel2 VARCHAR(20), IN pass VARCHAR(15))
BEGIN 
CASE op 
WHEN 'I' THEN 
     INSERT INTO `empleado` VALUES (id,ci,nom,sex,mail,cel1,cel2,pass);
     COMMIT;
WHEN 'U' THEN
     UPDATE `empleado` SET CI=ci, NOMBRES=nom,SEXO=sex,EMAIL=mail,CELULAR=tel1,CONVENCIONAL=tel2,CONTRASENA=pass 
     WHERE COD_EMPLEADO=id;
     SELECT * FROM `empleado` WHERE CI=ci;
WHEN 'D' THEN
     DELETE FROM `empleado` WHERE CI=ci;
     SELECT * FROM `empleado`;
WHEN 'S' THEN
     SELECT * FROM `empleado`;
WHEN 'C' THEN 
     SELECT * FROM `empleado` WHERE CI=ci;
END CASE;  
    
 END$$

CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `op_existencia_empleado`(IN username VARCHAR(100),IN pass varchar(25))
BEGIN 
declare total int;
DECLARE ex INT;
set ex=-1;
select cod_empleado into total from empleado where CI=username and contrasena=pass;
if total > 1 then
select distinct(cod_tipo) into ex from empleado_tipo where total=cod_empleado;
end if;
select ex;
  END$$

CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `pa_empleado`()
BEGIN 
select e.cod_empleado,e.ci,e.nombres,"hombre" as sexo,e.email,e.celular,e.convencional,te.descripcion as tipo
from empleado as e inner join empleado_tipo as et on e.cod_empleado=et.cod_empleado
inner join tipo_empleado as te on et.cod_tipo=te.cod_tipo
 where e.sexo='H'
union 
select e.cod_empleado,e.ci,e.nombres,"mujer" as sexo,e.email,e.celular,e.convencional,te.descripcion as tipo
FROM empleado AS e INNER JOIN empleado_tipo AS et ON e.cod_empleado=et.cod_empleado
INNER JOIN tipo_empleado AS te ON et.cod_tipo=te.cod_tipo
where e.sexo='M'
order by 2 asc;
END$$

CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `periodoMes`()
BEGIN
	DECLARE fec DATE;
	DECLARE fec1 DATE;
	SET @fec = '2014-01-01';
	SET @fec1 =ADDDATE(@fec,INTERVAL 1 month);
	WHILE @fec1 <= '2014-12-31' DO
	INSERT INTO periodo(COD_PERIODO,DESCRIPCION,FECHA_INICIO,FECHA_FIN)
		VALUES ('',NULL,@fec,@fec1);
		SET @fec = ADDDATE(@fec,INTERVAL 1 month);
		SET @fec1 = ADDDATE(@fec1,INTERVAL 1 month);
	END WHILE;
	
		select @fec,@fec1;
END$$

CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `periodos`()
BEGIN
	DECLARE fec DATE;
	DECLARE fec1 DATE;
	SET @fec = '2014-01-01';
	SET @fec1 =ADDDATE(@fec,INTERVAL 1 WEEK);
	WHILE @fec1 <= '2014-12-31' DO
	INSERT INTO periodo(COD_PERIODO,DESCRIPCION,FECHA_INICIO,FECHA_FIN)
		VALUES ('',NULL,@fec,@fec1);
		SET @fec = ADDDATE(@fec,INTERVAL 1 WEEK);
		SET @fec1 = ADDDATE(@fec1,INTERVAL 1 WEEK);
	END WHILE;
	
		select @fec,@fec1;
END$$

CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `porcentajeIndicadoresxEscuela`(in proc int,in macro int,in esc int,in  porcind float)
BEGIN
    
    DEclare maxim float;
    declare cumple float;
    set maxim=(select valor FROM escuela_proceso WHERE COD_PROCESO=proc AND COD_MACROPROCESO=macro AND COD_ESCUELA=esc);
    set cumple=(porcind*100)/maxim;
    
    select cumple;
    
    
    END$$

CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `PorcentajeReporteMensual`(IN proc INT,IN macro INT,IN esc INT,IN  porcind FLOAT,IN num INT)
BEGIN
    DECLARE maxim FLOAT;
    DECLARE cumple FLOAT;
    DECLARE porj FLOAT;
    SET porj=porcind/num;
    SET maxim=(SELECT valor FROM escuela_proceso WHERE COD_PROCESO=proc AND COD_MACROPROCESO=macro AND COD_ESCUELA=esc);
    SET cumple=(porj*100)/maxim;
    
    SELECT cumple;
    END$$

CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `ProcesosMensual`(IN macro INT,IN porc INT,IN codem INT,IN mes INT,in escod int)
BEGIN
	SELECT DISTINCT I.COD_PROCESO,I.FECHA_INICIO,I.FECHA_FIN,I.COD_MACROPROCESO,I.COD_EMPLEADO,I.PORCENTAJE 
	FROM indicador AS I INNER JOIN proceso AS P ON P.COD_PROCESO=I.COD_PROCESO INNER JOIN escuela_proceso AS ESP ON P.COD_PROCESO=ESP.COD_PROCESO
	WHERE I.COD_EMPLEADO=codem AND I.COD_PROCESO=porc AND ESP.COD_ESCUELA=escod  and I.COD_MACROPROCESO=macro AND ((YEAR(I.FECHA_INICIO)=YEAR(CURRENT_DATE) AND MONTH(I.FECHA_INICIO)=mes) OR (YEAR(I.FECHA_INICIO)=YEAR(CURRENT_DATE) AND MONTH(I.FECHA_FIN)=mes)) ;
	
    END$$

