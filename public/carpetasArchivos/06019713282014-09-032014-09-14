CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `nuevosValores`()
BEGIN 
DECLARE nempleados int;
DECLARE nprocesos INT;
DECLARE porcent FLOAT;
DECLARE nuevoValor float;
SET nempleados=(SELECT @nempleados := count(*) FROM empleado_escuela WHERE cod_escuela=2);
SET nprocesos=(SELECT @nprocesos := COUNT(*) FROM proceso WHERE cod_macroproceso=1);
SET porcent=(SELECT @porcent := porcentaje FROM macroproceso WHERE cod_macroproceso=1);
set nuevoValor=porcent/nprocesos/nempleados;
select nuevoValor;
  END$$

