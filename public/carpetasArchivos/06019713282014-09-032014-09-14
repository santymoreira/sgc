CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `AvanceContabilidadyAuditoria`()
BEGIN
		DECLARE sumatoria FLOAT;
DECLARE fecha1 INT; 
DECLARE fecha2 INT;
DECLARE i INT;
DECLARE porcema INT;
DECLARE resultado FLOAT;
DECLARE numeroprocesos INT;
DECLARE avance FLOAT;
declare macro int;
declare avancetotal float;
set macro=1;
WHILE macro<= 7 DO
 
 
SET i=1;
SET avance=0;
SET fecha1=(SELECT MONTH(CURRENT_DATE)); 
SET fecha2=(SELECT YEAR(CURRENT_DATE));
SET numeroprocesos=(SELECT COUNT(cod_proceso) FROM proceso WHERE cod_macroproceso=macro);
WHILE i<= numeroprocesos DO
SET sumatoria=(SELECT SUM(porcentaje) FROM indicador WHERE cod_macroproceso=macro AND cod_proceso=i AND MONTH(FECHA_INICIO)=fecha1 AND YEAR(FECHA_INICIO)=fecha2 AND MONTH(FECHA_FIN)=fecha1 AND YEAR(FECHA_FIN)=fecha2);
IF sumatoria > 0 THEN
	SET avance=avance+sumatoria;
END IF;
	
SET i=i+1;
END WHILE;
SET porcema=(SELECT porcentaje FROM macroproceso WHERE cod_macroproceso=macro);
SET resultado=(avance*100)/porcema;
		
set avancetotal=avancetotal+resultado;		
SET macro=macro+1;				
END WHILE;
SELECT avancetotal,fecha1,fecha2;
		
		
		
		
		
    END$$

CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `checkValor`(in escuela int,IN empleado INT,IN proceso INT,IN macroproceso INT,in f1 date,in f2 date)
BEGIN 
declare p1 float;
declare p2 int;
declare va float;
set va=(SELECT @va:=valor FROM escuela_proceso WHERE COD_ESCUELA=escuela AND COD_PROCESO=proceso AND COD_MACROPROCESO=macroproceso);
SET p1=(SELECT DISTINCT @p1 := porcentaje FROM indicador WHERE COD_EMPLEADO=empleado and COD_PROCESO=proceso and COD_MACROPROCESO=macroproceso and FECHA_INICIO=f1 and FECHA_FIN=f2);
if (p1=va) then set p2='1';
elseif p1=0 then set p2='0';
else set p2='2';
end if;
select p2;
  END$$

CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `consolidadoMacroprocesos`(IN macro INT, IN escod INT)
BEGIN
	DECLARE sumatoria FLOAT;
DECLARE fecha1 INT; 
DECLARE fecha2 INT;
DECLARE i INT;
DECLARE porcema INT;
DECLARE resultado FLOAT;
DECLARE numeroprocesos INT;
DECLARE avance FLOAT;
SET i=1;
SET avance=0;
SET fecha1=(SELECT MONTH(CURRENT_DATE)); 
SET fecha2=(SELECT YEAR(CURRENT_DATE));
SET numeroprocesos=(SELECT COUNT( DISTINCT P.cod_proceso) FROM proceso  AS P INNER JOIN escuela_proceso AS ESP ON ESP.COD_PROCESO=P.COD_PROCESO WHERE P.cod_macroproceso=macro AND ESP.COD_ESCUELA=escod);
WHILE i<= numeroprocesos DO
SET sumatoria=(SELECT SUM(porcentaje) FROM  (SELECT  DISTINCT  I.porcentaje,I.cod_proceso,I.cod_macroproceso,ESP.cod_escuela,I.cod_empleado,I.fecha_inicio,I.fecha_fin FROM proceso AS P INNER JOIN indicador AS I ON I.COD_PROCESO=P.COD_PROCESO INNER JOIN escuela_proceso AS ESP ON P.COD_PROCESO=ESP.COD_PROCESO WHERE I.cod_macroproceso=macro AND ESP.COD_ESCUELA=escod AND I.cod_proceso=i AND MONTH(I.FECHA_INICIO)=fecha1 AND YEAR(I.FECHA_INICIO)=fecha2 AND MONTH(I.FECHA_FIN)=fecha1 AND YEAR(I.FECHA_FIN)=fecha2) AS Tabla );
IF sumatoria > 0 THEN
	SET avance=avance+sumatoria;
END IF;
	
SET i=i+1;
END WHILE;
SET porcema=(SELECT DISTINCT M.porcentaje FROM macroproceso AS M  INNER JOIN proceso AS P ON M.COD_MACROPROCESO=P.COD_MACROPROCESO INNER JOIN escuela_proceso AS ESP ON P.COD_PROCESO=ESP.COD_PROCESO WHERE M.cod_macroproceso=macro AND ESP.COD_ESCUELA=escod );
SET resultado=(avance*100)/porcema;
SELECT resultado,fecha1,fecha2;
    
    END$$

