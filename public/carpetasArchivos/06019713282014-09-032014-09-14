-- --------------------------------------------------------

--
-- Estructura de tabla para la tabla `proceso`
--

CREATE TABLE IF NOT EXISTS `proceso` (
  `COD_PROCESO` int(11) NOT NULL,
  `DESCRIPCION` varchar(200) NOT NULL,
  `OBJETIVO` varchar(300) NOT NULL,
  `COD_MACROPROCESO` int(11) NOT NULL,
  `COD_FRECUENCIA` int(11) DEFAULT NULL,
  `COD_LUGAR` int(11) DEFAULT NULL,
  `COD_TIPO` int(11) NOT NULL,
  `TIPO_EMPLEADO` int(11) DEFAULT NULL,
  PRIMARY KEY (`COD_PROCESO`,`COD_MACROPROCESO`),
  KEY `COD_MACROPROCESO` (`COD_MACROPROCESO`),
  KEY `COD_LUGAR` (`COD_LUGAR`),
  KEY `proceso_ibfk_3` (`TIPO_EMPLEADO`),
  KEY `COD_FRECUENCIA` (`COD_FRECUENCIA`),
  KEY `COD_TIPO` (`COD_TIPO`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

