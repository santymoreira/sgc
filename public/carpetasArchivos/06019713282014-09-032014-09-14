CREATE DEFINER=`u679330740_root`@`localhost` PROCEDURE `funcionProcesos`(IN macroproceso INT,IN tipo INT)
BEGIN
SELECT PORCENTAJE FROM MACROPROCESO WHERE COD_MACROPROCESO=1
UNION
SELECT COUNT(*) FROM proceso WHERE COD_MACROPROCESO=macroproceso
UNION
SELECT COUNT(DISTINCT(cod_empleado)) FROM empleado_tipo WHERE cod_tipo=tipo;
    END$$

